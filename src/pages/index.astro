---
import PageMeta from "../components/PageMeta.astro";
import PageLayout from "../layouts/PageLayout.astro";
import Intro from "../components/Intro.astro";
import ContactForm from "src/components/ContactForm.astro";
import AboutTheTheme from "../components/AboutTheTheme.astro";
import Experiences from "src/components/Experiences.astro";
import Skills from "src/components/Skills.astro";
import { SITE_TITLE } from "../config";
---

<PageLayout>
  <PageMeta title={`${SITE_TITLE} | Personal Website`} slot="meta" />
  <Fragment slot="main">
    <Intro />
    <Experiences/>
    <Skills/>
    <ContactForm/>
  </Fragment>
</PageLayout>
